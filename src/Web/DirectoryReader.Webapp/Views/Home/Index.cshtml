@{
    ViewData["Title"] = "Home Page";
}

<form id="form" name="form" action="/api/File" enctype="multipart/form-data" method="post">
    <div class="buttons">
        <div class="upload-button">
            <div class="label">User name</div>
            <input type="text" id="txtUserName" name="UserName" class="content" />

            <div class="label">Password</div>
            <input type="password" id="txtPassword" name="Password" class="content" />

            <!--
    <div class="label">Upload File!</div>
    <input id="flUpload" name="files" type="file" size="1" accept=".zip" />
        -->
            <input type="submit" value="Submit" />
        </div>
    </div>
</form>

